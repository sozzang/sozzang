public class Star5 {
	public static void main(String[] args){
		for (int j = 0; j <= 5; j++){
		for(int z = 0; z < j; z++){
			System.out.print(" ");
		}	
		for (int i = 5; i > j; i--){
			System.out.print('*');
		}
			System.out.println();
		} 
	}
}